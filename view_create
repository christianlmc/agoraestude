cursos	CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `cursos` AS select `ae_site`.`ae_posts`.`ID` AS `id`,`ae_site`.`ae_posts`.`post_title` AS `nome` from `ae_site`.`ae_posts` where ((`ae_site`.`ae_posts`.`post_type` = 'product') and (`ae_site`.`ae_posts`.`post_status` = 'publish'))


curso_usuario	CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `curso_usuario` AS select distinct `ae_site`.`ae_woocommerce_order_itemmeta`.`meta_value` AS `curso_id`,`ae_site`.`ae_postmeta`.`meta_value` AS `usuario_id` from (((`ae_site`.`ae_posts` join `ae_site`.`ae_postmeta` on((`ae_site`.`ae_postmeta`.`post_id` = `ae_site`.`ae_posts`.`ID`))) join `ae_site`.`ae_woocommerce_order_items` on((`ae_site`.`ae_woocommerce_order_items`.`order_id` = `ae_site`.`ae_posts`.`ID`))) join `ae_site`.`ae_woocommerce_order_itemmeta` on((`ae_site`.`ae_woocommerce_order_itemmeta`.`order_item_id` = `ae_site`.`ae_woocommerce_order_items`.`order_item_id`))) where ((`ae_site`.`ae_posts`.`post_status` in ('wc-processing','wc-completed')) and (`ae_site`.`ae_woocommerce_order_itemmeta`.`meta_key` = '_product_id') and (`ae_site`.`ae_postmeta`.`meta_key` = '_customer_user'))


wp_usuarios	CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `wp_usuarios` AS select `ae_site`.`ae_users`.`ID` AS `id`,`ae_site`.`ae_usermeta`.`meta_value` AS `nome`,`ae_site`.`ae_users`.`user_login` AS `username`,`ae_site`.`ae_users`.`user_email` AS `email`,`ae_site`.`ae_users`.`user_pass` AS `password` from (`ae_site`.`ae_users` join `ae_site`.`ae_usermeta` on((`ae_site`.`ae_usermeta`.`user_id` = `ae_site`.`ae_users`.`ID`))) where (`ae_site`.`ae_usermeta`.`meta_key` = 'first_name')
